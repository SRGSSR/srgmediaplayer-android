<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SRGMediaPlayerView">
        <attr name="containerAspectRatio" format="string"/>
        <attr name="videoScale">
            <flag name="center_inside" value="0"/>
            <flag name="top_inside" value="1"/>
